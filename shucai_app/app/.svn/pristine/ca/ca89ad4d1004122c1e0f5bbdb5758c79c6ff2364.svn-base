.tabs {
    height: auto;
    border-bottom: 10px solid #EBECF0;
    width: 100%;
    overflow: hidden;
    margin: 0;
    background: #fff;
}

.tabs dd {
    height: 60px;
    line-height: 60px;
    font-size: 14px;
    padding: 0 1%;
    border-bottom: 4px solid #fff;
    float: left;
    overflow: hidden;
    position: relative;
    width: 16.5%;
    margin: 0;
    text-align: center;
}

.tabs dd a {
    display: block;
    outline: none;
}

.tabs dd a:hover {
    background: #ccc;
}

.tabs .thistab, .tabs .thistab a:hover {
    background: #fff;
    border-bottom: 4px solid #f29600;
    color: #F29600;
}

