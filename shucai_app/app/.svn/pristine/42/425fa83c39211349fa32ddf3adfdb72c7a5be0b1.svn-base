html,
body {
    height: 100%;
    margin: 0;

}


#slid {
    height: 36%;
}

.mui-slider-group {
    height: 100%;
}

.mui-slider-item {
    height: 100%;
    width: 100%;
    margin-left: 0px;
    margin-right: 0px;
    border: none;
}

a {
    height: 100%;
}

.img_content {
    position: relative;
    width: 100%;
    height: 100%;
}

.img_content img:first-child {
    position: absolute;
}

.img_content img:nth-child(2) {
    position: absolute;
    top: 30%;
}

.img_content img:last-child {
    position: absolute;
}

#button_a {

    position: absolute;
    display: block;
    bottom: 11%;
    width: 40%;
    height: 42px;
    left: 30%;
}

#button_a > img {
    display: inline-block;
    position: absolute;
}

.clearfix::before,
.clearfix::after {
    content: "";
    line-height: 0;
    height: 0;
    display: block;
    visibility: hidden;
    clear: both;
}

.swiper-container {
    background: #fff;
    min-height: 200px;
}

.swiper-container-horizontal > .swiper-pagination-bullets {
    bottom: 0px;
}

.swiper-pagination-bullet-active {
    background: #f29600;
}
#no_more {
    width: 100%;
    height: 54px;
}
.no_text {
