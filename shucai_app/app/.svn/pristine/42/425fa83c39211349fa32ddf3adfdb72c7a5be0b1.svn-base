/* 首页页 广告位 */

.floor_images {
    width: 100%;
    overflow: hidden;
    background: #FFF;
    border-top: 1px solid #eeeeee;
    margin-top: 10px;
}

.Edge {
    border-bottom: 1px solid #eeeeee;
    margin-top: -1px;
}

.floor_images ul {
    width: 100%;
    overflow: hidden;
    margin: auto;
    border-top: 1px solid #eeeeee;
}

.floor_images ul li {
    width: 50%;
    overflow: hidden;
    float: left
}

.floor_images ul li img {
    display: block;
    width: 100%;
    height: auto
}

.brom {
    border-right: 1px solid #eeeeee;
    margin-left: -1px;
}

.floor_images dl {
    width: 100%;
    overflow: hidden;
}

.floor_images dl img {
    display: block;
