/*
 * 搜索框样式
 */
.mui-search .mui-placeholder{
	color: #67BD49;
}
.mui-search:before
{
    font-family: Muiicons;
    font-size: 20px;
    color: #67BD49;
    font-weight: 900;

    position: absolute;
    top: 50%;
    right: 50%;

    display: none;

    margin-top: -18px;
    margin-right: 31px;

    content: '\e466';
}
.mui-search .mui-placeholder .mui-icon {
    font-size: 20px;
    font-weight: bolder;
    color: #67BD49;
    padding-right: 15px;
}
input[type=search] {
    border-radius: 15px;
    background-color: rgba(255,255,255,1);
}
.mui-bar-nav~.mui-content {
	padding-top:0 ;
    margin-top: 27px;
}

.mui-content {
    background: #F1F2F7;
    height: 100%;
    padding: 0;
    padding-bottom: 50px;

}
.mui-bar-nav~.mui-content {
    padding-top: 0;
    margin-top: 45px;
}


