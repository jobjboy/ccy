body {
    padding: 0px;
    margin: 0;
    font-size: 12px;
    line-height: 1.5em;
    font-family: tahoma, "Microsoft YaHei", Simsun, Mingliu, Arial, Helvetica;
    color: #333333;
    background: #FFFFFF;
    width: 100%;
}

img {
    border: 0;
    vertical-align: middle;
}

ul,
li {
    list-style-type: none;
    padding: 0;
    margin: 0;
}

table,
td {
    border-collapse: collapse
}

address,
em {
    font-style: normal;
}

ul, li a {
    color: #333333;
    text-decoration: none;
}

a:hover {
    color: #f29600;
    text-decoration: none;
}

cite, em, s, i, b {
    font-style: normal
}

.mui-bar h1 {
    color: #000;
    font-weight: 300;
}

#mui-color {
    background: #fff;
    color: #000;
    /*box-shadow: 0 0 0;*/
}

#mui-chat {
    transform: rotateY(180deg);
    -moz-transform: rotateY(180deg);
    -ms-transform: rotateY(180deg);
    /* IE 9 */
    -webkit-transform: rotateY(180deg);
    /* Safari 和 Chrome */
    -o-transform: rotateY(180deg);
    float: right;
}

.shopping {
    width: 100%;
    height: auto;
}

.shopping-title {
    width: 100%;
    height: 40px;
    line-height: 40px;
    padding: 0 15px;
    /*background: url(../images/shopping_title.png) 15px center no-repeat #e1e5e6;*/
    /*background-size: 28px 20px;*/
}

.shopping-news {
    padding-left: 35px;
    float: left;
}

.shopping-news li {
    height: 40px;
    line-height: 40px;
    font-size: 14px;
}

.shopping-news li a {
    color: #282828;
}

div#lodding {
    text-align: center;
    margin: 170px auto 0 auto;
    display: none;
}

.shopping-close {
    width: 24px;
    height: 24px;
    float: right;
    box-sizing: border-box;
    background: #282828;
    color: #fff;
    border-radius: 50%;
    margin-top: 8px;
}

#scrollDiv {
    width: 80%;
    height: 40px;
    overflow: hidden;
    float: left;
}

/*这里的高度和超出隐藏是必须的*/
#scrollDiv li {
    height: 40px;
    line-height: 40px;
    vertical-align: bottom;
    zoom: 1;
    font-size: 14px;
    padding-left: 35px;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    -webkit-text-overflow: ellipsis;
}

#scrollDiv li a {
    color: #333;
    text-decoration: none;
}

header {

}

#scrollDiv li a:hover {
    color: #FF0000;
    text-decoration: underline;
}

.shopping-list {
    padding: 0;
    margin: 0 0 0 0;
    margin-bottom: 100px;
    /*margin-top: 10px;*/
}

.shopping-list li {
    margin-bottom: 5px;
    background: #fff;
    /*padding: 15px 15px 0px 15px;*/
    overflow: hidden;
}

.shopping-list .shop-xz {
    width: 20px;
    height: 20px;
    background: #f2f6f7;
    border-radius: 50%;
    margin-right: 15px;
    border: 1px solid #b3b3b3;
    float: left;
    margin-top: 34px;
    margin-bottom: 34px;
}

/*.shop-xz,#all_select{ display: none;}*/
.shop-xz {
    /*display: none;*/
}

.ch {
    background: url(../images/index/gou.png) no-repeat;
    background-size: 100%;
    width: 18px;
    height: 18px;
    display: block !important;
    border-radius: 50%;
}

.shopping-list .shop-img {
    float: left;
    border: 1px solid #d6dadb;
    width: 100px;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-right: 15px;
}

.shopping-list .shop-img img {
    width: 100%;
}

.shopping-list .shop-title {
    /*margin-bottom: 30px;*/
    margin-left: 40px;
    /*height: 40px;*/
    word-break: break-all;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    font-size: 16px;
}
.peisong {
    color: red;
    display: inline-block;
    width: 47%;
    text-align: left;
    float: left;
}
.shopping-list .shop-title a {
    color: #282828;
    font-size: 14px;
}

.shopping-list .shop-jg {
    float: left;
    height: 25px;
    line-height: 25px;
    color: #fa8327;
    margin-top: 20px;
}

.shopping-list .mui-numbox {
    float: right;
}

.shopping-bottom {
    position: fixed;
    bottom: 50px;
    background: #fff;
    height: 50px;
    width: 100%;
    padding-left: 15px;
    display: none;
}

.shopping-bottom .shop-qx {
    width: 20px;
    height: 20px;
    background: #f2f6f7;
    border-radius: 50%;
    margin-right: 10px;
    border: 1px solid #b3b3b3;
    float: left;
    margin-top: 15px;
}

.shopping-bottom .shop-botton {
    width: 100px;
    height: 50px;
    text-align: center;
    line-height: 50px;
    color: #fff;
    background: #f29600;
    display: block;
    float: right;
}

.shopping-bottom > span {
    color: rgb(242, 150, 0);
    font-weight: 500;
    position: absolute;
    right: 110px;
    top: 15px;
}

/*footer_nav1底部悬浮菜单css*/
.v_nav {
    max-width: 100%;
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 50px;
    background-color: #FFFFFF;
    border-top: #dcdcdc 1px solid;
    z-index: 97;
    padding-top: 5px;
    margin: 0 auto;
    /*padding-bottom: 2px;*/
    /*padding-top: 2px*/
    box-sizing: border-box;
}

.vf_nav {
    width: 100%;
    height: 50px;
    margin: auto;
    padding: 0px;
    margin: 0px;
}

.vf_nav ul li {
    width: 25%;
    height: 50px;
    float: left;
    text-align: center;
    margin: 0px;
}

.vf_nav ul li a {
    text-align: center;
    font-size: 12px;
    color: #636363;
    width: 45px;
    display: block;
    height: 50px;
    position: relative;
    margin: auto
}

.vf_nav ul li i {
    display: block;
    width: 24px;
    height: 24px;
    margin: auto;
}

.vf_1 {
    /*background: url(../images/index/0.png);*/
    -webkit-background-size: 100%;
    background-size: 100%;

}

/*li:first-child span*/
/*{*/
/*color: #f29600;*/
/**/
/*}*/
.vf_2 {
    /*background: url(../images/index/1-0.png);*/
    -webkit-background-size: 100%;
    background-size: 100%;
}

.vf_3 {
    /*background: url(../images/index/2-0.png);*/
    -webkit-background-size: 100%;
    background-size: 100%;
}

.vf_4 {
    /*background: url(../images/index/3-0.png);*/
    -webkit-background-size: 100%;
    background-size: 100%;
}

.vf_nav ul li span {
    display: block;
    width: 84px;
    height: 20px;
    line-height: 20px;
    text-align: center;
    position: relative;
    left: 50%;
    margin-left: -42px
}

.vf_nav ul li a:active {
    color: #f29600;
}

/*new style*/
.c_text {
    display: inline-block;
    width: 40px;
    height: 30px;
    text-align: center;
    float: left;
    position: absolute;
    left: 10%;
    top: 15px;
    font-size: 14px;
}

.lmessage {
    display: inline-block;
    width: 24px;
    height: 23px;
    background: url(../images/index/message.png) no-repeat;
    -webkit-background-size: 100%;
    background-size: 100%;
    position: absolute;
    top: 12px;
    right: 4%;
}

.edit {
    color: rgb(100, 100, 100);
    display: inline-block;
    width: 40px;
    height: 23px;
    position: absolute;
    top: 12px;
    right: 15%;
    font-size: 14px;
    font-weight: 100;
    display: none
}

.shop-collect {
    width: 100px;
    height: 50px;
    text-align: center;
    line-height: 50px;
    color: #fff;
    background: #f29600;
    display: none;
    float: right;
    margin-right: 0.5%;
}

.shop-delete {
    width: 100px;
    height: 50px;
    text-align: center;
    line-height: 50px;
    color: #fff;
    background: #dd6a08;
    display: none;
    float: right;
}

.fail {
    text-align: center;
    position: relative;
    background-color: #fff;
}

.fail span {
    background: url(../images/noorder.png) no-repeat center;
    background-size: 35%;
    display: block;
    height: 200px;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
}

.fail_text {
    padding: 0;
    margin: 0;
    color: #333;
    display: inline-block;
    position: relative;
    top: 68%;
    font-size: 16px;
}

.shopping-list .numbox {
    right:0px;
    position: absolute;
    bottom:0px;
    display: inline-block;
    overflow: hidden;
    width: 80px;
    height: 25px;
    border-radius: 3px;
    background-color: #fff;
}

.numbox span {
    font-weight: bold;
    background: #fff;
    display: inline-block;
    /*border:1px solid #ccc;*/
    width: 22px;
    text-align: center;
    position: absolute;
    border-radius: 100%;
    height: 22px;
    /*color: rgb(242,150,0);*/
    /*line-height: 22px;*/

}

.numbox .minus {
    background: url("../images/minus.png");
    -webkit-background-size: 100%;
    background-size: 100%;
}

.numbox .plus {
    background: url("../images/add.png");
    -webkit-background-size: 100%;
    background-size: 100%;
}

.numbox .num {
    display: inline-block;
    text-align: center;
    vertical-align: middle;
    position: relative;
    height: 100%;
    margin-bottom: 0px;
    position: relative;
    bottom: 0px;
    left: 27%;
    color: rgb(0,0,0);
}

.numbox input[type=number] {
    padding: 0;
    width: 36px;
    border: none;
    color: rgb(0,0,0);
}

.numbox span:nth-of-type(2) {
    right: 0;
}

#goShopping {
    display: inline-block;
    width: 80px;
    height: 30px;
    border: 1px solid #9fa1a0;
    border-radius: 5px;
    line-height: 30px;
    /*position: absolute;*/
}
.returnBack {
    position: absolute;
    top: -4px;
    left: 10px;
    width:54px;
    height:54px;
    text-align: center;
    border-radius: 50%;
    background: url(../images/index/left.png) no-repeat 5px 15px	;
    background-size: 46%;
    z-index: 10;
    padding: 15px;
    color: #fff;
    display: none;
}
.vf_nav ul li .car_count {
    display: inline-block;
    display: none;
    position: absolute;
    top: -2px;
    right: -4px;
    z-index: 100;
    background: red !important;
}
