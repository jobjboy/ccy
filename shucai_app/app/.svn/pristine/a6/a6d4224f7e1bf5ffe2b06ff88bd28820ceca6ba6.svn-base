    -webkit-transform: rotate(45deg); /* for Chrome || Safari */
    -moz-transform: rotate(45deg); /* for Firefox */
    -ms-transform: rotate(45deg); /* for IE */
    -o-transform: rotate(45deg); /* for Opera */
    transform: rotate(45deg);

}

