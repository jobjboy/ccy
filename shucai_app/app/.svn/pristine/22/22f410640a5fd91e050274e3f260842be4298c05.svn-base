/*密码框*/
.bg {
    width: 100%;
    height: 100%;
    overflow: hidden;
    background: rgba(0,0,0,0.3);
    /*display: none;*/
    position: fixed;
    transition: all 0.5s;
    z-index: 50;
    top: 0px;
}

.inputbar {
    margin: 0 auto;
    width: 80%;
    /*height: 36%;*/
    display: block;
    float: left;
    background: #f7f7f7;
    position: fixed;
    top: 36%;
    left: 10%;
    /*margin-left: -200px;*/
    transition: all 0.5s;
    z-index: 1000;
    text-align: center;
    padding: 5px;
    /*background: url("../images/fenge.png") repeat-x;*/
    /*background-size: 1%;*/
}
.pincode-input-container {
    /*display:inline-block;*/
    margin-top: 21%;
    margin-left: 9%;
    margin-bottom: 10px;
}
.pincode-input-container input.first {
    border-top-right-radius:0px;
    border-bottom-right-radius:0px;
}
.pincode-input-container input.last {
    border-top-left-radius:0px;
    border-bottom-left-radius:0px;
    border-left-width:0px;
}
.pincode-input-container input.mid {
    border-radius:0px;
    border-left-width:0px;
}
.pincode-input-text, .form-control.pincode-input-text {
    width: 15%;
    float: left;
    text-align: center;
}
.pincode-input-error{
    clear:both;
}
