    margin-right: 5px;
    margin-top: 18px;
    float: left;
}

.geng {
    width: 50px;
    height: 25px;
    float: right;
    padding-top: 5px;
}

.geng a {
    display: block;
    float: left;
    width: 30px;
    line-height: 25px;
    height: 25px;
    color: #7f7f7f;
    font-size: 13px;
}

.geng span {
    display: block;
    float: left;
    width: 11px;
    height: 20px;
    background: url(../images/index/jt.png) no-repeat;
    background-size: 75%;
    margin-top: 4px;
    margin-left: 3px;
}

.floor_body .b_box {
    width: 95%;
    overflow: hidden;
    margin: auto;
    clear: both;
}

.floor_body .b_box div {
    width: 30%;
    float: left;
    margin-bottom: 10px;
    margin-left: 0.2%;
}

.floor_body .b_box div:first-child {
    width: 66%;
    float: left;
    margin-bottom: 10px;
}

.floor_body .b_box div:nth-child(2) {
    width: 32.5%;
    float: right;
    margin-bottom: 10px;
}

.floor_body .b_box div:nth-child(n+3) {
    width: 30%;
    float: left;
    margin-bottom: 10px;
    margin-left: 2%;
}

.floor_body .b_box img {
    width: 100%;
    /*height: auto;*/
    height: 118px;
}

.floor_body .b_box div:first-child img {
    width: 49%;
    height: auto;
    position: relative;
    left: 50%;
    margin-left: -25%;
    height: 118px;
}

.floor_body .b_box .price {
    width: 100%;
    overflow: hidden;
    margin: auto;
    /*height: 33px;*/
    display: inline-block;
}

.floor_body .b_box .title {
    display: inline-block;
    word-break: break-all;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    height: 35px;
    line-height: 17px;
}

.floor_body .price {
    font-size: 12px;
}

.floor_body .price_color {
    color: #f09c28;
}

.floor_body2 h2 {
    height: 30px;
    line-height: 30px;
    font-size: 16px;
    font-weight: normal;
    color: #A8A8A8;
    text-align: center;
    background-color: #EAECEF;
