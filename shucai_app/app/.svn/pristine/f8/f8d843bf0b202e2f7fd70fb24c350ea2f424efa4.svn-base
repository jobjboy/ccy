ul,
li {
    list-style-type: none;
}

.mui-bar {
    position: fixed;
    z-index: 10;
    right: 0;
    left: 0;

    height: 44px;
    padding-right: 10px;
    padding-left: 10px;
    color: #636363;
    border-bottom: 0;
    background-color: #ffffff;

}

#mui-color {
    background: #fff;
    box-shadow: 0 0 0;
    border-bottom: 1px solid #f4f4f6;
}

#topPopover {
    position: fixed;
    top: 16px;
    right: 10px;
    width: 150px;
}

#topPopover .mui-popover-arrow {
    left: auto;
    right: 10px;
}

p {
    text-indent: 22px;
}

.mui-popover {
    height: 190px;
}
div.mui-popover2 {
    height: 110px;
}

.mui-content {
    padding: 10px;
}

.mui-tc {
    padding: 0 10px;
    margin: 0 auto;
}

.mui-tc li {
    border-bottom: 1px solid #acacac;
    padding: 10px;
}

.mui-tc li a {
    color: #292929;
}

.mui-tc li a span {
    margin-right: 10px;
}

.more {
    float: right;
    text-align: center;
    z-index: 10;
    color: #000;
}

.add-list {
    width: 100%;
    margin-top: 44px;
    background: #F1F2F6;
    overflow-y: auto;  height:80%; margin-bottom:50px;
}

.add-list ul {
    padding: 0;
    margin: 0;
}

.add-list ul li {
    width: 100%;
    background: #fff;
    box-sizing: border-box;
    overflow: hidden;
    position: relative; margin-bottom:8px;
}
.add-list ul li:active{ background:#eee;}
div#lodding{ text-align: center; margin:50px  auto 0 auto;  display: none;}

.add-list ul li .mui-checkbox.mui-radio input {
    left: 0px;
}

.add-list ul li .mui-radio {
    width: 120px;
    float: left;
    height: 50px;
    margin-left: 2px;
}

.add-list-left {
    float: left;
     padding: 8px 10px 6px;
    height: auto;
    line-height: 18px;
    overflow: hidden;
    color: #1B1B1B;

}

.add-list-title {
    float: left;  font-size: 15px;color: #050505;
}

.add-list-tel {
    margin-left:18px; float: left; color: #050505; font-size: 15px;
}

.add-list-text {
    float: left;
    width: 100%; color: #686868;
    line-height: 25px; font-size: 12px; margin-top:3px;
}


.add-list-line{border-top: 1px solid #F4F4F4; width: 100%; clear: both;  margin:0 auto; font-size: 13px;}
.add-list-line div.mui-checkbox{margin-left: -15px;}

.add-list-line label{ margin-top:6px;}

.add-list-line{border-top: 1px solid #F4F4F4; width: 100%; clear: both;  margin:0 auto; font-size: 13px;}
.add-list-line div.mui-radio{margin-left: -15px;}

.add-list-line label{ margin-top:6px;}
.mui-radio.mui-left input[type=checkbox], .mui-radio.mui-left input[type=radio] {
    top:17px;
}
.mui-radio input[type=checkbox]:checked:before, .mui-radio input[type=radio]:checked:before {
    color: #F29600;
}

.mui-radio.mui-left label, .mui-radio.mui-left label {
    padding-left: 38px; color: #7A7A7A; margin-top:11px;
}
#addressList{
    position:relative;
}
#addressList span,#addressList a.delButton{
    display: inline-block;
    position:absolute;
    bottom:25px;
    height: 0px; font-size: 16px;
}
#addressList span.addButton{   right:90px;}
#addressList a.delButton{   right:20px; font-size: 16px; color: #A1A1A3; }
#addressList a.delButton m{ font-size: 12px; margin-left:-5px;}

#addressList span a{ font-size: 13px; color: #A1A1A3; line-height: 1;}
.mui-radio input[type=checkbox]:before, .mui-radio input[type=radio]:before {
    font-size: 22px;

}

.address-button-m{

    position: fixed; display: block; width: 100%;
    bottom:0px; background: #F1F2F6; z-index: 4; height: 60px;

}
.address-button {
    width: 90%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    color: #fff;
    background: #F29600; display: block;
    border: none; margin: 10px auto;
    padding: 0;


}


.mui-radio.mui-left input[type=radio] {
    left: 10px;
}

.mui-radio input[type=radio]:checked:before {
    color: #f29600;
}

.mui-icon-back {
    color: #333;
}


.ys {
    width: 24px;
    height: 24px;
    border-radius: 50%;
    border: 1px solid #9fa1a0;
    position: absolute;
    top: 14px;
}
.xz {
    background-color: #f29600;
    border:none;
}

#zanwu {
    text-align: center;
    margin-top: 100px;
    font-size: 16px;
    /*margin:0 auto;*/
    display: none;
}



