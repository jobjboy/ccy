* {
    margin: 0;
    padding: 0;
}

body {
    padding: 0px;
    font-size: 12px;
    line-height: 1.5em;
    font-family: tahoma, "Microsoft YaHei", Simsun, Mingliu, Arial, Helvetica;
    color: #333333;
    background: #f7f7f7;
    width: 100%;
    overflow-x: hidden;
}

img {
    border: 0;
    vertical-align: middle;
}

ul,
li {
    list-style-type: none;

}

table,
td {
    border-collapse: collapse
}

address,
em {
    font-style: normal;
}

a {
    color: #999 !important;
    text-decoration: none;
}

/*.shop_car_text {
	color: #999999;
}*/

a:hover {
    color: #f29600;
    /*text-decoration: none;*/
}

/*通用样式结束*/
/*头部样式*/
.mui-bar {
    position: fixed;
    z-index: 10;
    right: 0;
    left: 0;
    height: 44px;
    padding-right: 10px;
    padding-left: 10px;
    border-bottom: 0;
    background-color: #ffffff;
    box-shadow: none;
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;
    display: none;
}

.mui-bar-nav {
    top: 0;
    border-bottom: 1px solid #E3E3E3;
}

.mui-title {
    font-weight: 300;
}

.message {
    display: inline-block;
    width: 24px;
    height: 22px;
    background: url(../images/message.fw.png);
    -webkit-background-size: 100%;
    background-size: 100%;
    position: absolute;
    top: 12px;
    right: 4%;
}

.returnBack {
    position: fixed;
    top: 15px;
    left: 20px;
    width: 54px;
    height: 54px;
    text-align: center;
    border-radius: 50%;
    background: url(../images/index/left.png) no-repeat 8px 11px;
    background-size: 40%;
    z-index: 10;
    padding: 15px;
    color: #fff;
}

.more {
    position: fixed;
    right: 10px;
    top: 10px;
    width: 54px;
    height: 54px;
    text-align: center;
    padding: 15px;
    border-radius: 50%;
    background: url(../images/index/message.png) no-repeat center center;
    -webkit-background-size:40%;
    background-size:50%;
    z-index: 10;
    color: #fff;
}

.mui-plus .plus {
    display: inline;
}

.plus {
    display: none;
}

#topPopover {
    position: fixed;
    top: 16px;
    right: 10px;
    width: 150px;
}

#topPopover .mui-popover-arrow {
    left: auto;
    right: 10px;
}

p {
    text-indent: 22px;
}

.mui-popover {
    height: 190px;
}

.mui-content {
    padding: 10px;
}

.mui-tc {
    padding: 0 10px;
}

.mui-tc li {
    border-bottom: 1px solid #acacac;
    padding: 10px;
}

.mui-tc li a {
    color: #292929;
}

.mui-tc li a span {
    margin-right: 10px;
}

.pro-tilte {
    padding: 15px; padding-top: 40px;
    background: #fff;
    font-size: 18px;
    position: relative; line-height: 1.2;
}

.pro-text {
    padding: 10px 15px;
    background: #fff;
    color: red;
    font-size: 24px;
}

.pro-text a {
    color: red !important;
}

.pro-text span {
    float: right;
    color: #767676;
    font-size: 16px;
}

.pro-standard {
    padding: 20px 15px;
    background: #fff;
    margin-bottom: 5px;
}

.pro-standard span {
    border: 1px solid #959595;
    padding: 5px;
    cursor: pointer;
    border-radius: 5px;
    color: #848484;
    margin-right: 5px;
}

.pro-standard span.pro-standard-active {
    background: #f19700;
    color: #fff;
}

.pro-amount {
    padding: 15px;
    background: #fff;
    margin-bottom: 5px;
}

.pro-foods {
    padding: 15px;
    background: #fff;
    margin-bottom: 5px;
    position: relative;
}

.pro-foods span {
    color: #828282;
}

.pro-foods a {
    color: #000;
}

.pro-condition {
    padding: 10px;
    background: #fff;
    margin-bottom: 5px;
    position: relative;
}

.pro-condition span {
    color: #f19700;
    border: 1px solid #f19700;
    border-radius: 50%;
    margin-right: 5px;
}

.pro-condition ul {
    overflow: hidden;
}

.pro-condition li {
    float: left;
    font-size: 14px;
    color: #828282;
    margin-right: 1%;
    margin-left: 2%;
}

.pro-evaluate {
    padding: 7px;
    background: #fff;
    position: relative;
    border: 1px solid rgb(232, 233, 237);
}

.pro-evaluate a {
    color: #000;
}

.pro-detail {
    max-width: 100%;
    background: #fff;
    margin: 10px 0 40px;
    z-index: 1;
}

.pro-detail img {
    width: 100%;
}

.pro-detail-top {
    padding: 15px;
    color: #828282;
    background: #fff;
    border-radius: 50%;
    border: 1px solid #555;
    position: fixed;
    bottom: 80px;
    z-index: 10;
    right: 10px;
}

.pro-detail-buy {
    width: 25%;
    height: 60px;
    padding: 10px 0;
    background: red;
    float: right;
    text-align: center;
    line-height: 20px;
    color: #FFF
}

/*.pro-detail-buy a{background: url(../images/immediately_buy_icon.png) left center no-repeat; padding-left: 45px; width: auto; height:40px; color: #fff; display:inline-block; font-size: 24px; background-size: 40px;}*/
.pro-detail-buy a {
    color: #fff;
}

.pro-detail-cart {
    width: 25%;
    height: 60px;
    padding: 10px 0;
    float: right;
    background: #f29600;
    text-align: center;
    line-height: 20px;
    color: #FFF
}

/*.pro-detail-cart a{ width: auto; height:40px; color: #fff; display:inline-block; font-size: 24px; background-size: 40px;line-height: 40px;}*/
.mui-icon-extra-cart:before {
    font-size: 30px;
}

.pro-detail-footer {
    background: #fff;
    overflow: hidden;
    position: fixed;
    left: 0px;
    bottom: 0px;
    width: 100%;
    height: 40px;
    font-size: 15px;
}

.fixednav {
    display: none
}

.mui-table-view-cell {
    /*margin: 5px 0;*/
    border: none !important;
    margin-bottom: 5px;
}

.mui-table-view, .mui-collapse, .mui-collapse-content, .mui-navigate-right {
    /*border: none !important;*/
}

.mui-collapse-content #guige {
    border-bottom: none !important;
}

.mui-slider {
    min-height: 375px;
    background: #FFF;
}

.detail_footer_left {
    width: 50%;
    color: #999;
    border-top: 1px solid #E5E5E5;
    float: left;
}

.collect {
    width: 33%;
    height: 40px;
    text-align: center;
    float: left;
}

em.btm-act-icn {
    width: 23px;
    height: 21px;
    display: block;
    margin: 0 auto;
    background-color: #fff;
    margin-top: 10px;
    margin-bottom: 4px;
    background: url(../images/cart_sprits_all.png?v=1) 0 0 no-repeat;
    background-size: 100px 100px;
    position: relative;
    background-position: -26px 0;
    background-size: 100px 100px;
    width: 19px;
}

 span.focus-info{
    display: block;
    line-height: 9px;
    height: 15px;
    text-align: center;
}

.star {
    width: 20px;
    height: 20px;
    display: inline-block;
    position: relative;
    top: 2px;
    left: 13px;
    background: url(../images/order/star.png) no-repeat;
    background-size: 100%;
}

.star_ye {
    background: url(../images/order/star_ye.png) no-repeat;
    background-size: 100%;
}

.collect_text {
    display: inline-block;
    position: relative;
    top: 15px;
    right: 14px;
    font-size: 12px;
}

.shop_car {
    width: 40%;
    height: 40px;
    text-align: center;
    position: relative;
    float: left; margin-left: -20px;
}

.shop_car_img {
    display: inline-block;
    width: 20px;
    height: 20px;
    position: relative;
    top: 4px;
    left: 24px;
    background: url(../images/select_shop_icon.png) no-repeat;
    -webkit-background-size: 100%;
    background-size: 100%;
}

.shop_car_text {
    display: inline-block;
    position: relative;
    top: 15px;
    right: 6px;
    font-size: 12px;
}

.navTmp a {
    color: #999;
}

a:visited {
    color: #999
}

/*回到顶部*/
.back_top {
    width: 46px;
    height: 46px;
    background: url(../images/index/back_top_button@3x.png) no-repeat;
    -webkit-background-size: 100%;
    background-size: 100%;
    position: fixed;
    bottom: 95px;
    right: 6%;
    opacity: 0.5;
    display: none;
}

.numbox {
    margin-right: 3%;
    float: right;
    position: relative;
    display: inline-block;
    overflow: hidden;
    width: 80px;
    height: 25px;
    vertical-align: top;
    vertical-align: middle;
    border-radius: 3px;
    background-color: #fff;
}

.numbox span {
    font-weight: bold;
    background: #fff;
    display: inline-block;
    /*border:1px solid #ccc;*/
    width: 22px;
    text-align: center;
    position: absolute;
    border-radius: 100%;
    height: 22px;
    /*color: rgb(242,150,0);*/
    /*line-height: 22px;*/

}

.numbox .minus {
    background: url("../images/minus.png");
    -webkit-background-size: 100%;
    background-size: 100%;
}

.numbox .plus {
    background: url("../images/add.png");
    -webkit-background-size: 100%;
    background-size: 100%;
}

.numbox .num {
    display: inline-block;
    text-align: center;
    vertical-align: middle;
    position: relative;
    height: 30px;
    position: relative;
    bottom: 4px;
    left: 28%;
}

.numbox input[type=number] {
    padding: 0;
    width: 36px;
    border: none;
}

.numbox span:nth-of-type(2) {
    right: 0;
    top: 0;
}

.share {
    position: absolute;
    background: url(../images/index/share.png) no-repeat 0px 0px;
    background-size: 100%;
    width: 24px;
    height: 24px;
    right: 12px;
}

.pro_all {
    background: #fff;
}

.evaluate_des {
    padding: 0 15px;
    font-size: 16px;
    color: #767676;
}

.evaluate_des li {
    padding: 5px 0;
    font-size: 14px;
}

.evaluate_des li.des_title {
    position: relative;
    font-size: 16px;
}

.evaluate_des li.des_title span {
    position: absolute;
    right: 0;
    font-size: 14px;
    font-size: 14px;
}

.mui-collapse-content {
    background-color: #fff;
}

.mui-navigate-right:after, .mui-push-right:after {
    right: 2% !important;
}

.car_count {
    display: inline-block;
    /*border:1px solid #000;*/
    display: none;
    position: absolute;
    top: 0px;
    right: 12px;
    z-index: 100;
    background: red !important;
}

.bj {
    background: url(../images/pro-detail/zanwu.png) no-repeat;
    -webkit-background-size: 100%;
    background-size: 100%;
}

#rentout {
    height: 40px;
    width: 90%;
    line-height: 18px;
    /*margin: 0 auto;*/
    background: #f29600;
    color: #ffffff;
    position: fixed;
    left: 50%;
    margin-left: -45%;
    display: none;
    /*top:-40px;*/
    bottom: 0;
}

.pro_all a {
    width: 100%;
    height: 100%;
    display: block;
}
.seemore {
    display: none;
    padding-top: 10px;
    padding-left: 45%;
}