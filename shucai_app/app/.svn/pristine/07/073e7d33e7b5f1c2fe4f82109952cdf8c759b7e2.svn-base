* {
	margin: 0;
	padding: 0;
}

body {
	padding: 0px;
	font-size: 12px;
	line-height: 1.5em;
	font-family: tahoma, "Microsoft YaHei", Simsun, Mingliu, Arial, Helvetica;
	color: #333333;
	background: #f7f7f7;
	width: 100%;
	overflow-x: hidden;
	background-color: #fff;
}

img {
	border: 0;
	vertical-align: middle;
}

ul,
li {
	list-style-type: none;
}

table,
td {
	border-collapse: collapse
}

address,
em {
	font-style: normal;
}

a {
	color: #333333;
	text-decoration: none;
}

a:hover {
	color: #828282;
	text-decoration: none;
}
a:active {
	background-color: #E3E3E3;
}

cite, em, s, i, b { font-style: normal }

/*通用样式结束*/


/*header样式*/

.mui-bar-nav {
	background: -webkit-linear-gradient(#dd6a80, #f29600);
	/* Safari 5.1 - 6.0 */
	background: -o-linear-gradient(#dd6a80, #f29600);
	/* Opera 11.1 - 12.0 */
	background: -moz-linear-gradient(#dd6a80, #f29600);
	/* Firefox 3.6 - 15 */
	background: linear-gradient(#dd6a80, #f29600);
	/* 标准的语法 */
	overflow: hidden;
}

.message {
	display: inline-block;
	width: 24px;
	height: 23px;
	background: url(../images/index/home_message_icon@3x.png);
	-webkit-background-size: 100%;
	background-size: 100%;
	position: absolute;
	top: 12px;
	right: 4%;
}

.scan_icon {
	display: inline-block;
	width: 24px;
	height: 23px;
	background: url(../images/index/home_scan_icon@3x.png);
	-webkit-background-size: 100%;
	background-size: 100%;
	margin-top: 12px;
}

#search_icon {
	display: inline-block;
	width: 15px;
	height: 14px;
	background: url(../images/index/home_search_icon@3x.png);
	-webkit-background-size: 100%;
	background-size: 100%;
	position: absolute;
	top: 12px;
	right: 6%;
}

.mui-bar form {
	width: 70%;
	position: relative;
	top: -36px;
	left: 14.44%;
}

.mui-bar form #search_bar {
	border-radius: 20px;
	background-color: #EE9A29;
	color: #F6C98E;
	text-align: left;
}


/*分类导航*/

.entry-list {
	width: 100%;
	/*overflow: hidden;*/
	padding-top: 10px;
	background: #FFF;
	padding-bottom: 10px;
}

.entry-list #itemsNav {
	width: 100%;
	overflow: hidden;
	margin: auto
}

.entry-list a {
	display: block;
	width: 25%;
	overflow: hidden;
	float: left;
	text-align: center;
	margin-top: 5px;
}

.entry-list a span {
	height: 30px;
	line-height: 30px;
	color: #616161;
	font-size: 14px;
}

.entry-list a img {
	display: block;
	width: 65%;
	margin: auto;
}


/*底部模块*/

.footer {
	background-color: #F8F8F8;
	text-align: center;
	width: 100%;
	overflow: hidden;
	padding-bottom: 10px;
	padding-top: 10px;
}

.footer a {
	display: inline-block;
	/*padding: 0 1em*/
}

.footer span {
	color: #82858A;
}

.footer .tip {
	padding: 0 0 1.5em
}

.footer .tip span {
	font-size: 1.2em
}

.footer .links {
	width: 75%;
	margin: 0 auto;
	background: none
}

.footer .links a {
	border-right: 1px solid #dcdcdc;
	height: 20px;
	line-height: 20px;
	font-size: 14px;
	padding-left: 10px;
	padding-right: 10px;
}

.footer .links a:last-child {
	border-right: 0 none
}

.footer .linkss {
	width: 65%;
	margin: 0 auto;
	height: 70px;
	margin-top: 10px;
}

.footer .linkss li {
	width: 33.3%;
	float: left;
	text-align: center;
	height: 70px;
	font-size: 10px;
}

.footer .linkss li a i {
	display: block;
	width: 40px;
	height: 40px;
	margin: auto;
}

.footer .gl {
	color: #c30e0e
}

.footer p {
	text-align: center;
	line-height: 20px;
}

.mf_o4 {
	color: #a2a2a2;
	font-size: 10px;
}


/* 首页页 广告位 */

.floor_images {
	width: 100%;
	overflow: hidden;
	background: #FFF;
	border-top: 1px solid #eeeeee;
	margin-top: 10px;
}

.Edge {
	border-bottom: 1px solid #eeeeee;
	margin-top: -1px;
}

.floor_images ul {
	width: 100%;
	overflow: hidden;
	margin: auto;
	border-top: 1px solid #eeeeee;
}

.floor_images ul li {
	width: 50%;
	overflow: hidden;
	float: left
}

.floor_images ul li img {
	display: block;
	width: 100%;
	height: auto
}

.brom {
	border-right: 1px solid #eeeeee;
	margin-left: -1px;
}

.floor_images dl {
	width: 100%;
	overflow: hidden;
}

.floor_images dl img {
	display: block;
	width: 100% !important;
	height: auto !important
}

.floor_images dl dt {
	width: 50%;
	float: left;
	overflow: hidden;
	border-right: 1px solid #eeeeee;
	margin-left: -1px;
}

.floor_images dl dd {
	width: 50%;
	float: left;
	overflow: hidden
}

.floor_images dl dd span {
	display: block;
	width: 100%;
	overflow: hidden;
}

.floor_images strong {
	display: block;
	width: 100%;
	overflow: hidden;
	margin: auto;
	border-top: 1px solid #eeeeee;
	clear: both;
}

.floor_images strong img {
	display: block;
	width: 100%;
	height: auto;
}

.banner img
{
	width: 100%;
	height: auto;
}


/*楼层recommend_promotion+cat_goods*/

.index_floor_lou {
	width: 100%;
	overflow: hidden;
	margin-top: 10px;
	background: #FFF;
	border-bottom: 1px solid #dcdcdc
}

.floor_body {
	width: 100%;
	overflow: hidden;
}

.floor_body h2 {
	height: 35px;
	line-height: 35px;
	font-size: 16px;
	font-weight: normal;
	color: #7f7f7f;
	margin-bottom: 10px;
	text-align: center;
}

.floor_body h2 em {
	width: 60px;
	height: 1px;
	background: #a5a5a5;
	margin-right: 5px;
	margin-top: 18px;
	float: left;
}

.geng {
	width: 50px;
	height: 25px;
	float: right;
	padding-top: 5px;
}

.geng a {
	display: block;
	float: left;
	width: 30px;
	line-height: 25px;
	height: 25px;
	color: #7f7f7f;
	font-size: 13px;
}

.geng span {
	display: block;
	float: left;
	width: 11px;
	height: 20px;
	background: url(../images/index/more.png) no-repeat;
	background-size: 75%;
	margin-top: 4px;
	margin-left: 3px;
	
}

.floor_body .b_box {
	width: 95%;
	overflow: hidden;
	margin: auto;
	clear: both;
}

.floor_body .b_box div {
	width: 30%;
	float: left;
	margin-bottom: 10px;
	margin-left: 0.2%;
}

.floor_body .b_box div:first-child {
	width: 66%;
	float: left;
	margin-bottom: 10px;
}

.floor_body .b_box div:nth-child(2){
	width: 32.5%;
	float: right;
	margin-bottom: 10px;
}

.floor_body .b_box div:nth-child(n+3) {
	width: 30%;
	float: left;
	margin-bottom: 10px;
	margin-left: 2%;
}

.floor_body .b_box  img {
	width: 100%;
	/*height: auto;*/
	height: 118px;


}
.floor_body .b_box div:first-child img
{
	width: 49%;
	height: auto;
	position: relative;
	left: 50%;
	margin-left: -25%;
	height: 118px;

}

.floor_body .b_box .price {
	width: 100%;
	overflow: hidden;
	margin: auto;
	/*height: 33px;*/	
	display: inline-block;
	
}

.floor_body .b_box .title
{
	display: inline-block;
	word-break: break-all;
  	display: -webkit-box;
  	-webkit-line-clamp: 2;
  	-webkit-box-orient: vertical;
  	overflow: hidden;
  	height: 35px;
  	line-height: 17px;

}
.floor_body .price {
	font-size: 12px;
}

.floor_body .price_color {
	color: #f09c28;
}


.floor_body2 h2 {
    height: 30px;
    line-height: 30px;
    font-size: 16px;
    font-weight: normal;
    color: #A8A8A8;
    text-align: center;
    background-color: #EAECEF;
    margin: 0;
}



/* 去掉链接触摸高亮 */
.index_banner .bd li .tit {
	display: block;
	width: 100%;
	position: absolute;
	bottom: 0;
	text-indent: 10px;
	height: 28px;
	line-height: 28px;
	color: #fff;
	text-align: left;
}
/*商品信息*/
.items_info
{
	font-size: 14px;
	font-family: "Adobe 黑体 Std";
	font-weight: 900;
}


