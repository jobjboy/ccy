    margin-top: 10px;
    background: #FFF;
    border-bottom: 1px solid #dcdcdc
}

.floor_body {
    width: 100%;
    overflow: hidden;
}

.floor_body h2 {
    height: 35px;
    line-height: 35px;
    font-size: 16px;
    font-weight: normal;
    color: #7f7f7f;
    margin-bottom: 10px;
    text-align: center;
}

.floor_body h2 em {
    width: 60px;
