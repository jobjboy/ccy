ul,
li {
	list-style-type: none;
}
a {
	text-decoration: none;
	color: #000;
}
#mui-color{ background: #fff; box-shadow: 0 0 0; border-bottom: 1px solid #f4f4f6; }

.describe{ width: 100%; height: auto; overflow: hidden; padding: 10px 15px 0 15px; margin-top: 53px; background: #fff;}
.describe-title{ width: 100%;  box-sizing: border-box; height: 50px; line-height: 40px; border-bottom: 1px solid #e4e4e4; }
.describe-title img{ width: 40px; height: 40px; border: 1px solid #e4e4e4; border-radius: 5px; float: left; margin-right: 10px;}
.describe-title span{ float: right; display: block; color: #a3a3a3; font-size: 14px;}
.describe-title em{ float: left; font-style: normal;font-size: 14px;}

.describe-tb{ float: left; padding: 0; margin: 10px 0 0;}
.describe-tb li{ float: left; width: 20px; height: 20px; margin-left: 10px; cursor: pointer;}
.describe-tb li img{ max-width: 100%; height: 20px; border: none;
}

.filter{
    -webkit-filter: grayscale(100%);
    -moz-filter: grayscale(100%);
    -ms-filter: grayscale(100%);
    -o-filter: grayscale(100%);
    filter: grayscale(100%);
    filter: gray;
}

.describe-tb li.checkall-ys img{filter: grayscale(0);}
.describe-tb span {margin-left: 20px;margin-top: 5px !important;}


.describe-write{ width: 100%; height: auto; margin-top: 10px; border: none; color: #000; font-size: 14px; padding: 0;min-height: 100px;}
.describe-add{ width: 100px; height: 100px;float: left;margin-right: 0.6rem;margin-bottom: 0.6rem;}
.describe-add a{ display: block; border: 1px solid #E4E4E4; width: 100px; height: 100px; color: #E4E4E4; text-align: center; border-radius: 3px;}
.describe-add em{ width: 100%; font-size: 50px; text-align: center; margin-top: 10px;}

.describe-anonymity{ width: 100%; margin-top: 15px; float: left; text-indent: 2.2em; height: 50px; line-height: 50px; font-size: 14px; border-top: 1px solid #E4E4E4; }
.describe .mui-checkbox.mui-left input[type=checkbox]{ left: 0; margin-top: 7px;}
.describe-anonymity span{ float: right; color: #a3a3a3;}

.graded{ width: 100%; padding: 0px 15px 10px; background: #fff; margin-top: 10px;}
.graded h2{ padding: 0; border-bottom: 1px solid #E4E4E4; height: 50px; font-weight: normal; margin: 0 0 10px 0; line-height: 50px; font-size: 16px;  color: #282828;}
.graded h3{ padding: 0; height: 40px; font-size: 16px;font-weight: normal;  margin: 0;}
.graded h3 span{ float: left; margin-top: 12px;}

.evaluate-button{ width: 90%; height: 50px; border: none; text-align: center; margin: 10px auto; display: block; background: #F29600; color: #fff; font-size: 20px;}

.up_file{position: relative;}
.up_file input{
	position: absolute;
	width: 100px;
	font-size: 80px;
	right: 0;
	top: 0;
	opacity: 0	;
    overflow: hidden;
	text-align: center !important;
	display: table-cell;
	vertical-align: middle;
}
.mui-checkbox input[type=checkbox]:checked:before, .mui-radio input[type=radio]:checked:before{
	    color: #f0ad4e;
}
